    "from sklearn.linear_model import LinearRegression\n",
    "X = [[1],[2],[3],[4],[5]]\n",
    "y = [30,40,50,65,70]\n",
    "\n",
    "f_lr = LinearRegression()\n",
    "\n",
    "f_lr.fit(X,y)\n",
    "\n",
    "hours = int(input(\"Enter your study hours: \"))\n",
    "f_lr_prediction = f_lr.predict([[hours]])\n",
    "print(f_lr_prediction)"
   ]
